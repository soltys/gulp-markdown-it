{
    "name": "@psoltys/gulp-markdown-it",
    "version": "0.2.0",
    "description": "A plugin for gulp to render markdown with markdown-it",
    "keywords": [
        "gulp",
        "gulpplugin",
        "markdown",
        "markdown-it"
    ],
    "repository": "soltys/gulp-markdown-it",
    "author": {
        "name": "Pawel Soltysiak",
        "email": "pawel@soltysiak.it",
        "url": "https://blog.soltysiak.it/en/"
    },
    "files": [
        "index.js"
    ],
    "scripts": {
        "prepublish": "gulp coffee --require coffee-script/register",
        "test": "coffeelint gulpfile.coffee index.coffee test -f ./coffeelint.json && istanbul test _mocha --report lcov --report html -- ./test/*.coffee --require coffee-script/register --reporter spec",
        "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
    },
    "dependencies": {
        "gulp-util": "^3.0.0",
        "through2": "^3.0.0",
        "underscore": "^1.9.1",
        "markdown-it": "^8.x"
    },
    "devDependencies": {
        "coffee-script": "^1.12.7",
        "coffeelint": "^2.1.0",
        "coveralls": "^3.0.2",
        "del": "^3.0.0",
        "gulp": "^4.0.0",
        "gulp-bump": "^3.1.1",
        "gulp-coffee": "^3.0.3",
        "gulp-git": "^2.8.0",
        "gulp-tag-version": "^1.3.1",
        "gulp-util": "^3.0.8",
        "istanbul": "^0.4.5",
        "markdown-it-checkbox": "^1.1.0",
        "mocha": "^5.2.0",
        "should": "^13.2.3"
    },
    "engines": {
        "node": ">=0.10.0",
        "npm": ">=1.3.7"
    },
    "license": "MIT"
}
